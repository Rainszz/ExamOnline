<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ExamOnline05</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.yhl.ex.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QuestionManageServlet</servlet-name>
    <servlet-class>com.yhl.ex.servlet.QuestionManageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QuestionManageServlet</servlet-name>
    <url-pattern>/questionManage</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>EnCodeFilter</filter-name>
    <filter-class>com.yhl.ex.filter.EnCodeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EnCodeFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.yhl.ex.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/jsp/*</url-pattern>
  </filter-mapping>
</web-app>